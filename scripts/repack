#!/bin/sh

set -o pipefail -o errexit -o nounset

cd "$(dirname "$0")/.."

./scripts/build

out=monty-on-the-run.retro.tap

rm -f "$out"

taput \
  add -b -o 10 -n "MOTR" motr/motr.bas "$out"
taput \
  add -o 16384 -n "MOTR1" motr/motr.img "$out"
taput \
  add -o 24576 -n "MOTR2" out/motr.patched.bin "$out"

taput list "$out"
